<script type="text/javascript">
	window.onload = function(){
		//body 영역에서 마우스 버튼 눌렀을 때 작동되는 이벤트
		document.body.onmousedown = function(){
			console.log('body 에서 마우스 버튼 누름');
		};
		
	};
</script>
