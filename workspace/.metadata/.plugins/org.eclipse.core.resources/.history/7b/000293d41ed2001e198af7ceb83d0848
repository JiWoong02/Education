package test.synch;

public class Account {
	private static int balance; // 통장 잔액

	public Account() {
	}

	// 예금 인출하는 메소드 : 통장 잔액 차감 처리
	public void withDraw(int money) {
		Thread.sleep(100);
		
		if (balance >= money) {
			this.balance -= money;
		}
	}

	public static int getBalance() {
		return balance;
	}

	public static void setBalance(int balance) {
		Account.balance = balance;
	}
	
	
	
	
	
}
